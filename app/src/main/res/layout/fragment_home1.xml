<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F8F8FF">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="342dp"
        android:layout_height="180dp"
        android:background="@drawable/layout_round"
        app:layout_constraintLeft_toRightOf="@id/guide_vocSe"
        app:layout_constraintTop_toBottomOf="@id/guide_totalVoc"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="319dp"
            android:layout_height="123dp"
            app:srcCompat="@drawable/voc_2"
            tools:layout_editor_absoluteX="-9dp"
            tools:layout_editor_absoluteY="-5dp" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="20dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide_go"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="238dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide_baro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="248dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="만족 37%"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintLeft_toRightOf="@id/guide_1"
        app:layout_constraintTop_toBottomOf="@id/guide_percent"
        tools:layout_editor_absoluteX="15dp" />

    <TextView
        android:id="@+id/textView61"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="만족 54%"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintLeft_toRightOf="@id/guide_2"
        app:layout_constraintTop_toBottomOf="@id/guide_percent"
        tools:layout_editor_absoluteX="15dp" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="만족 72%"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintLeft_toRightOf="@id/guide_3"
        app:layout_constraintTop_toBottomOf="@id/guide_percent"
        tools:layout_editor_absoluteX="15dp" />

    <TextView
        android:id="@+id/txt_vocGo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="> 고객의 소리 남기기"
        android:textSize="13sp"
        app:layout_constraintLeft_toRightOf="@id/guide_go"
        app:layout_constraintTop_toBottomOf="@id/guide_baro"
        tools:ignore="MissingConstraints" />

        tools:layout_editor_absoluteX="215dp"
        tools:layout_editor_absoluteY="145dp" />


    <include
        android:id="@+id/toolbar_home1"
        layout="@layout/toolbar_user" />

    <Button
        android:id="@+id/btn_scan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginTop="516dp"
        android:backgroundTint="@color/button2"
        android:text="QR 스캔"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide_totalVoc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="98dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide_vocSe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="10dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide_percent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="215dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="37dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="151dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="269dp" />

</androidx.constraintlayout.widget.ConstraintLayout>